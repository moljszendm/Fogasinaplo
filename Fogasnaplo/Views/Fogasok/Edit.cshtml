@model Fogasnaplo.Models.Fogas

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h4>Adatok</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Nev" class="control-label"></label>
                <input asp-for="Nev" class="form-control" />
                <span asp-validation-for="Nev" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CsapatNev" class="control-label"></label>
                <input asp-for="CsapatNev" class="form-control" />
                <span asp-validation-for="CsapatNev" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ido" class="control-label"></label>
                <input asp-for="Ido" class="form-control" />
                <span asp-validation-for="Ido" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HalFajtak" class="control-label"></label>
                <input asp-for="HalFajtak" class="form-control" />
                <span asp-validation-for="HalFajtak" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Suly" class="control-label"></label>
                <input asp-for="Suly" class="form-control" />
                <span asp-validation-for="Suly" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Allasok" class="control-label"></label>
                <input asp-for="Allasok" class="form-control" />
                <span asp-validation-for="Allasok" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Mentés" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="nav-link-1" asp-action="Index">Vissza a fogásokhoz</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
